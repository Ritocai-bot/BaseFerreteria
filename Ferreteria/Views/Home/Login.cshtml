@model Ferreteria.Clases.Sesiones

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>

    <script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />

    <link href="~/CSS/Header_Estilo.css" rel="stylesheet" type="text/css" />
    <link href="~/CSS/Footer_Estilo.css" rel="stylesheet" type="text/css" />
    <link href="~/CSS/Index_Estilo.css" rel="stylesheet" type="text/css" />
    <link href="~/CSS/Login_Estilo.css" rel="stylesheet" type="text/css" />

</head>
<body>
    <div class="loginbox">
        <img src="~/Imagenes/Paquete1/png/089-customer service.png" class="userA">
        
        <form action="~/Home/VerificarLogin" method="post">
            <div class="form-group">
                <label for="username:" class="control-label">UserName</label>
                <input name="NameUser" type="text" class="form-control" id="caja" title="Nombre Usuario" placeholder="Nombre Usuario" />
                <span class="help-block"></span>
            </div>

            <div class="form-group">
                <label for="password:" class="control-label">Password</label>
                <input name="Password" type="password" class="form-control" id="caja" title="Contrasena Usuario" placeholder="Contraseña" />
                <span class="help-block"></span>
            </div>
            <button type="submit" class="btn1">Enviar</button>
        </form>

</div>


  
</body>
</html>


